{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          blockMeshDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 1;

vertices        
(
    (0.000 0.00 0.000)
    (5e-5 0.00 0.000)
    (5e-5 5e-5 0.000)
    (0.000 5e-5 0.000)
    (0.000 0.00 5e-5)
    (5e-5 0.00 5e-5)
    (5e-5 5e-5 5e-5)
    (0.000 5e-5 5e-5)
);

blocks          
(
    hex (0 1 2 3 4 5 6 7) (20 20 20) simpleGrading (1 1 1)
);

edges           
(
);

patches         
(
    wall walls 
    (
        (4 5 6 7)
        (3 2 1 0)
        (1 5 4 0)
        (3 7 6 2)
        (0 4 7 3)
        (2 6 5 1)
    )
);

