FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.0001";
    object      Omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    bottom_wall
    {
        type            calculated;
        value           uniform 0;
    }
    top_wall
    {
        type            calculated;
        value           uniform 0;
    }
    right_wall
    {
        type            calculated;
        value           uniform 0;
    }
    left_wall
    {
        type            calculated;
        value           uniform 0;
    }
    defaultFaces
    {
        type            empty;
    }
}


