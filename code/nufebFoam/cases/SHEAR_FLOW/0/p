FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           volScalarField;
    object          p;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    top_wall           
    {
        type            zeroGradient;
    }

    bottom_wall          
    {
        type            zeroGradient;
      
     // type            fixedValue;
      //  value           uniform 0;
    }

    right_wall           
    {
      //  type            zeroGradient;
	type            fixedValue;
        value           uniform 0;
    }
    
    left_wall
    {
      //  type            zeroGradient;
	type            fixedValue;
        value           uniform 0e1;
	
    }
    

    defaultFaces    
    {
        type            empty;
    }
}


