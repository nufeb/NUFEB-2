# NUFEB simulation

atom_style	bio
atom_modify	map array sort 1000 5.0e-7
boundary	ff ff ff
newton		off
#processors  2 1 1

#radius x y z outerradius sub o2 nh4 no2 no3
communicate single vel yes
#read_data initial_cells_biofilm.in
read_data initial_cells_floc.in

group HET type 1
group AOB type 2
group NOB type 3 
group EPS type 4
group inert type 5
group dead type 6

neighbor	5.0e-7 bin
neigh_modify	delay 0 one 5000

#pair_style none
pair_style  gran/hooke/history 0.000200 NULL 0.000050 NULL 0.0 1

pair_coeff  * *
timestep 20

#velocity all set 0.0 0.0 0.0 units box

fix 1 all nve/limit 1e-8 

#fix	 2 all gravity 1.47 vector 0 0 -1 # spherical 90.0 -180.0 

fix 10 all viscous 0.1 


fix xw all wall/gran 2000 NULL 500.0 NULL 1.5 0 xplane  0.000000e-04 1.000000e-04
fix yw all wall/gran 2000 NULL 500.0 NULL 1.5 0 yplane  0.000000e-04 1.000000e-04
fix zw all wall/gran 2000 NULL 500.0 NULL 1.5 0 zplane  0.000000e-04 1.000000e-04

variable kanc equal 50

fix xwa all walladh v_kanc xplane  0.000000e-04 1.000000e-04
fix ywa all walladh v_kanc yplane  0.000000e-04 1.000000e-04
fix zwa all walladh v_kanc zplane  0.000000e-04 1.000000e-04

##############Ibm Variable Definitions##############

#Defining affinity variables
variable KsHET equal 0.01
variable Ko2HET equal 0.81
variable Kno2HET equal 0.0003
variable Kno3HET equal 0.0003

variable Knh4AOB equal 0.001
variable Ko2AOB equal 0.0005

variable Kno2NOB equal 0.0013
variable Ko2NOB equal 0.00068

#Defining maximum growth variables 
variable MumHET equal 0.000069
variable MumAOB equal 0.0000088
variable MumNOB equal 0.000009375
variable etaHET equal 0.6

#Defining decay rates variables
variable bHET equal 0.0000046296296
variable bAOB equal 0.000001273148
variable bNOB equal 0.000001273148
variable bEPS equal 0.000001967592


#Defining death and maintenance variables
variable bmHET equal 0.0
variable bmAOB equal 0.0
variable bmNOB equal 0.0
variable bX equal 0.0

#Defining yield coefficient variables
variable YHET equal 0.61
variable YAOB equal 0.33
variable YNOB equal 0.083
variable YEPS equal 0.18
variable Y1 equal 0.4

#Defining EPS density and ratio variables
variable EPSdens equal 30
variable EPSratio equal 1.25

#Defining diffusion coefficient variables
variable Do2 equal 0.000000002
variable Dnh4 equal 0.0000000014
variable Dno2 equal 0.0000000012
variable Dno3 equal 0.0000000012
variable Ds equal 0.0000000005


#Defining diffusion time variable
variable diffT equal 0.0001

#Defining inlet concentrations for non-ghost cells
variable sub equal  8.00e-03
variable o2 equal 10.00e-04
variable no2 equal 8.000e-04
variable no3 equal 8.0000e-04 
variable nh4 equal 9.00e-03

#Defining inlet concentrations for boundary conditions
variable BCsub equal 8.00e-03
variable BCo2 equal 10.00e-04
variable BCno2 equal 8.000e-04
variable BCno3 equal 8.0000e-04 
variable BCnh4 equal 9.00e-03

#Defining critical diameter of death
variable deadDia equal 0.8e-6

variable factor equal 1.5

variable ke equal 5e+9

#Defining shear force parameters
variable viscosity equal 0.001
variable shearRate equal 0.1
variable height equal 5e-5

##############Ibm functions##############

fix g1 all diffnugrowth 1 200 v_KsHET v_Ko2HET v_Kno2HET v_Kno3HET v_Knh4AOB v_Ko2AOB v_Kno2NOB v_Ko2NOB v_MumHET v_MumAOB v_MumNOB v_etaHET v_bHET v_bAOB v_bNOB v_bEPS v_bmHET v_bmAOB v_bmNOB b_bX v_YHET v_YAOB v_YNOB v_YEPS v_Y1 v_EPSdens v_Do2 v_Dnh4 v_Dno2 v_Dno3 v_Ds v_diffT v_sub v_o2 v_no2 v_no3 v_nh4 10 10 10 dirich v_BCsub v_BCo2 v_BCno2 v_BCno3 v_BCnh4 500

fix j1 all epsadh 1 v_ke 1
fix d1 all divide 500 v_EPSdens 2.0 3123
fix e1 HET eps_extract 500 v_EPSratio v_EPSdens 1242242
fix d5 all death 500 v_deadDia

#fix s1 all shear 10 v_viscosity v_shearRate v_height zx 0 80000

##############Simulation outputs#########
dump            id all custom 500 snapshot.bubblemd id type diameter mass x y z sub o2 no2 no3 nh4

#thermo_style   custom step atoms mhet maob mnob meps mdead
thermo_style    custom step atoms nhet naob nnob neps ndead
thermo		500
thermo_modify	lost ignore

run  36000
	

